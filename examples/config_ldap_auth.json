{
    "routes": {
        "Images": ["jpg", "jpeg", "png", "gif", "bmp", "tiff", "svg", "webp", "heic"],
        "Music": ["mp3", "wav", "flac", "aac", "ogg", "wma", "m4a"],
        "Videos": ["mp4", "mkv", "avi", "mov", "wmv", "flv", "webm"],
        "Documents": ["pdf", "doc", "docx", "txt", "rtf", "odt", "xls", "xlsx", "ppt", "pptx", "csv"],
        "Archives": ["zip", "rar", "7z", "tar", "gz", "bz2"],
        "Executables": ["exe", "msi", "bat", "cmd", "ps1"],
        "Shortcuts": ["lnk", "url"],
        "Scripts": ["py", "js", "html", "css", "json", "xml", "sh", "ts", "php"],
        "Fonts": ["ttf", "otf", "woff", "woff2"],
        "Logs": ["log"],
        "Other": []
    },
    "memory_threshold_mb": 200,
    "cpu_threshold_percent": 60,
    "logs_dir": "./logs",
        "downloads_json": "./config/json/downloads_dashboard.json",
        "file_moves_json": "./config/json/file_moves.json",
        "file_hashes_json": "./config/json/file_hashes.json",
        "vt_cache_json": "./config/json/vt_cache.json",
    
    "watch_folders": [],
    "vt_api_key": "",
    
    "dashboard_user": "admin",
    "dashboard_pass_hash": "$2b$12$...",
    
    "auth_method": "ldap",
    "auth_fallback_enabled": true,
    
    "ldap_config": {
        "server": "ldaps://ad.company.com:636",
        "base_dn": "dc=company,dc=com",
        "user_dn_template": "cn={username},cn=users,dc=company,dc=com",
        "use_ssl": true,
        "bind_dn": "",
        "bind_password": "",
        "search_filter": "(sAMAccountName={username})",
        "allowed_groups": [
            "cn=IT Staff,ou=Security Groups,dc=company,dc=com",
            "cn=Administrators,cn=Builtin,dc=company,dc=com"
        ]
    },
    
    "windows_auth_config": {
        "domain": "",
        "allowed_groups": []
    }
}
